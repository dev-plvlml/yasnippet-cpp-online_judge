using ordered_stringset = trie<string, null_type, trie_string_access_traits<>,
                               pat_trie_tag, trie_order_statistics_node_update>;

template <typename T>
using ordered_stringmap = trie<string, T, trie_string_access_traits<>,
                               pat_trie_tag, trie_order_statistics_node_update>;
